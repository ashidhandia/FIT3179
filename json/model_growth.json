{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "AI model sizes over time.",
  "width": 950,
  "height": 400,
  "background": "black",
  "padding": {"top": -70, "left": 10, "right": 10, "bottom": 0},
  "data": {
    "url": "https://raw.githubusercontent.com/ashidhandia/FIT3179/main/Data/ModelSize.csv"
  },
  "transform": [
    {"calculate": "toDate(datum.Year + '-01')", "as": "Date"},
    {
      "calculate": "datum['Parameters (B)'] * 1000",
      "as": "ParametersInMillions"
    }
  ],
  "encoding": {
    "x": {
      "field": "Date",
      "type": "temporal",
      "title": "",
      "axis": {
        "format": "%Y",
        "labelExpr": "year(datum.value)",
        "values": [
          "2018-01-01",
          "2019-01-01",
          "2020-01-01",
          "2021-01-01",
          "2022-01-01",
          "2023-01-01",
          "2024-01-01"
        ],
        "grid": false,
        "labelColor": "white"
      },
      "scale": {"domain": ["2018-01-01", "2025-01-01"]}
    },
    "y": {
      "field": "Parameters (B)",
      "type": "quantitative",
      "title": "Model Size (in Billions of Parameters)",
      "axis": {
        "format": "f",
        "tickCount": 5,
        "grid": false,
        "labelColor": "white",
        "titleColor": "white"
      },
      "scale": {"type": "log", "domain": [0.01, 2000], "nice": false}
    },
    "tooltip": [
      {"field": "Model", "title": "Model"},
      {"field": "Lab", "title": "Lab"},
      {
        "field": "ParametersInMillions",
        "title": "Size (Millions)",
        "format": ",.0f"
      }
    ]
  },
